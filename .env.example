# Copy this to .env and fill in your keys
OPENAI_API_KEY=sk-...
# Optional: choose a model. Defaults to gpt-4o-mini if omitted.
OPENAI_MODEL=gpt-4o-mini

# Set to 1 to force offline fallback (no LLM). This will use rule-based
# classification, template-based drafting, and a simple reviewer.
# Recommended only for demo without API keys.
DRY_RUN_NO_LLM=0
